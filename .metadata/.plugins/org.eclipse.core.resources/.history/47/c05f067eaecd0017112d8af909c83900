package test;

import java.util.Scanner;

public class Main {
	public static void main(String args[])
	{
		int m,n;
		Scanner scan=new Scanner(System.in);
		n=scan.nextInt();
		m=scan.nextInt();
		int map[][]=new int[n+1][n+1];
		for(int i=1;i<=m;i++)
		{
			int s=scan.nextInt();
			int e=scan.nextInt();
			map[s][e]=1;
		}
		for(int i=1;i<=n;i++)
		{
			for(int j=1;j<=n;j++)
			{
				for(int k=1;k<=n;k++)
				{
					if(map[i][j]==1&&map[j][k]==1)
					{
						map[i][k]=1;
					}
				}
			}
		}
		int nums[]=new int[n+1];
		int nume[]=new int[n+1];
		int num=0;
		for(int i=1;i<=n;i++)
		{
			for(int j=1;j<=n;j++)
			{
				if(map[i][j]==1)
				{
					nums[i]++;
				}
				if(map[j][i]==1)
				{
					nume[i]++;
				}
			}
			if(nums[i]==n||nume[i]==n)
			{
				num++;
			}
		}
		System.out.println(num);
	}
}
