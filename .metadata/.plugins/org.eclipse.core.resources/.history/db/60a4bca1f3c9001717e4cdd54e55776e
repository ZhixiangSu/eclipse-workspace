package timu2;

import java.util.Scanner;

public class timu2 
{
	static int a[]=new int[100000];
	public static void main(String args[])
	{
		Scanner scan=new Scanner(System.in);
		String s=new String();
		s=scan.nextLine();
		int sum=0,sum1=0,num=0;
		for(int i=0;i<s.length();i++)
		{
			if(s.charAt(i)=='£¬')
			{
				num++;
				a[num]=sum;
				sum1+=sum;
				sum=0;
			}
			else
			{
				sum*=10;
				sum+=s.charAt(i)-'0';
			}
		}
		num++;
		a[num]=sum;
		sum1+=sum;
		System.out.print(sum1/num);
	}
}
